    "iconv",
    "intl",
    "json",
    "mbstring",
    "pcre",
    "PDO",
    "pdo_mysql",
    "pdo_sqlite",
    "Reflection",
    "regex",
    "SimpleXML",
    "SPL",
    "Redis",
    "standard",
    "superglobals",
    "session",
    "xml",
    "xmlreader",
    "xmlwriter",
    "Zend OPcache"
